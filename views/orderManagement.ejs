	<!-- CONTENT -->
	<section class="content" id="blur">
		<!-- NAVBAR -->	
		<main class="content-main">		
            
            <div class="product-nav" style="display: flex;justify-content: space-between;"> 
		 <h3 style="font-size: 20px;text-transform: uppercase; font-weight: 500;">order Management</h3>

            </div>

				<br>
	
                <table class="table table-striped">
                    <thead>
                        <tr>
                          <th scope="col">No</th>
                          <th scope="col">Name</th>
                          <th scope="col">User</th>
                          <th scope="col">Payment status</th>
                          <th scope="col">status</th>
                          <th scope="col">quantity</th>
                          <th scope="col">total</th>
                          <th scope="col">Action</th>
                        
                            
                        </tr>
                      </thead>
                      <tbody>
                        <% orders.forEach((item,index)=>{ %>
                
                          <tr>
                            <th scope="row"><%= index+1%> </th>
                            <td><%= item.product.name  %></td>
                            <td><%=item.address.name%> </td>
                            <td>
                                <% if(item.paid){ %> Paid

                               <% }else { %> not paid <% } %>
                            </td>
                              
                            <td><%= item.orderStatus %></td>

                            <td ><%= item.quantity %></td>

                            <td><%= item.total %></td>
                           
                           
                            <td>
                                <div style="display: flex; align-items: center; gap: 10px;">

                                    <a href="/admin/edit-order/<%=item._id%>" style="background-color: rgb(88, 88, 195); color: white; padding: 10px 40px; font-size: 18px; font-weight: 300;">Edit</a>

                                </div>
                            </td>
                     
                          </tr>
                
                        <% }) %> 
                       
                       
                      </tbody>
                  </table>

		
		
         

		</main>

